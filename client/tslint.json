{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  },
  "rules": {
    "member-access": true,
    "interface-name": true,
    "no-console": false,
    "no-empty": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false
  }
}
